    <properties>
        <java.version>1.8</java.version>
        <kotlin.version>1.3.72</kotlin.version>
        <xc.version>2.0.0-SNAPSHOT</xc.version>
        <jsf.version>1.7.3</jsf.version>
        <mybatis.plus.version>3.3.2</mybatis.plus.version>
